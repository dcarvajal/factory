{
	"name": "@ivoryio/kogaio",
	"version": "0.4.5",
	"author": "Ivoryâ„¢",
	"description": "Component library",
	"main": "build/index.js",
	"module": "build/index.js",
	"license": "ISC",
	"publishConfig": {
		"access": "public"
	},
	"homepage": "https://github.com/ivoryio/factory/tree/master/packages/kogaio#introduction",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ivoryio/factory/tree/master/packages/kogaio"
	},
	"bugs": {
		"url": "https://github.com/ivoryio/factory/issues"
	},
	"scripts": {
		"start": "react-scripts start",
		"styleg": "npx styleguidist server --config src/config/styleguide.config.js",
		"build:lib": "NODE_ENV=production && rm -rf build && mkdir build && npx babel ./src/lib --out-dir ./build --ignore \"**/*.test.js,**/*.examples.md\" --copy-files",
		"publish:lib": "yarn build:lib && cp package.json build && cp README.md build && cd ./build && yarn publish --access public",
		"build:styleg": "rm -rf build && npx styleguidist build --config src/config/styleguide.config.js",
		"publish:styleg": "yarn build:styleg && aws s3 sync ./build s3://kogaio.ivory.io --profile default",
		"lint": "node_modules/.bin/eslint ./",
		"test": "react-scripts test",
		"eject": "react-scripts eject"
	},
	"devDependencies": {
		"@babel/cli": "^7.4.3",
		"@babel/core": "^7.4.3",
		"@babel/plugin-proposal-class-properties": "^7.4.0",
		"@babel/preset-env": "^7.4.3",
		"@babel/preset-react": "^7.0.0",
		"babel-eslint": "9.0.0",
		"babel-plugin-styled-components": "^1.10.0",
		"cz-conventional-changelog": "^2.1.0",
		"eslint": "5.12.0",
		"eslint-plugin-babel": "^5.3.0",
		"eslint-plugin-react": "^7.12.4",
		"husky": "^1.3.1",
		"lint-staged": "^8.1.5",
		"react": "^16.8.6",
		"react-docgen-external-proptypes-handler": "^1.0.2",
		"react-dom": "^16.8.6",
		"react-scripts": "^2.1.8",
		"react-styleguidist": "^9.0.8"
	},
	"dependencies": {
		"styled-components": "4.2.0",
		"styled-system": "^4.1.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"eslint --fix",
			"git add ."
		]
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
