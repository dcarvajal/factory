{
  "name": "@ivoryio/kogaio",
  "version": "0.7.13",
  "author": "MihaiRo <mihai.rosca@thinslices.com>",
  "description": "Component library",
  "main": "build/index.js",
  "module": "build/index.js",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/ivoryio/factory/tree/master/packages/kogaio#introduction",
  "repository": {
    "type": "git",
    "url": "https://github.com/ivoryio/factory/tree/master/packages/kogaio"
  },
  "bugs": {
    "url": "https://github.com/ivoryio/factory/issues"
  },
  "scripts": {
    "start": "react-scripts start",
    "styleg": "npx styleguidist server --config src/config/styleguide.config.js --open",
    "build:lib": "NODE_ENV=production && rm -rf build && npx babel src/lib --out-dir build -i \"**/*.test.js,**/*.examples.md\" && cp package.json README.md build",
    "publish:lib": "yarn build:lib && cd build && yarn publish --access public",
    "build:styleg": "rm -rf build && npx styleguidist build --config src/config/styleguide.config.js",
    "publish:styleg": "yarn build:styleg && aws s3 sync ./build s3://kogaio.ivory.io --profile ivory",
    "lint": "eslint '**/*.{js,jsx}'",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "babel-plugin-styled-components": "^1.10.6",
    "react": "^16.8.6",
    "react-docgen-external-proptypes-handler": "^1.0.3",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.1",
    "react-styleguidist": "^9.1.13",
    "styled-components": "^4.3.2"
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "styled-components": "^4.3.2"
  },
  "dependencies": {
    "@styled-system/prop-types": "^5.0.16",
    "styled-system": "^5.0.16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix",
      "git add ."
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
